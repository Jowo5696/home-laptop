snippet 'tikzcoordsys(\d)(\d)(\d)(\d)(\d)(\d)(\d)(\d)' "coordinate system" riA
\begin{tikzpicture}
\draw[->](`!p snip.rv=match.group(1)`,`!p snip.rv=match.group(2)`) -- (`!p snip.rv=match.group(3)`,`!p snip.rv=match.group(4)`) node[anchor=north west] {$1};
\draw[->](`!p snip.rv=match.group(5)`,`!p snip.rv=match.group(6)`) -- (`!p snip.rv=match.group(7)`,`!p snip.rv=match.group(8)`) node[anchor=south east] {$2}; $0
\end{tikzpicture}
endsnippet

snippet 'tikzcoordnsys(\d)(\d)(\d)(\d)(\d)(\d)(\d)(\d)' "coordinate system numbers" riA
\begin{tikzpicture}
\draw[->](`!p snip.rv=match.group(1)`,`!p snip.rv=match.group(2)`) -- (`!p snip.rv=match.group(3)`,`!p snip.rv=match.group(4)`) node[anchor=north west] {$1};
\foreach \x in {`!p 
xaxis=[]
x=abs(int(match.group(1)))+abs(int(match.group(3)))
for a in range(x):
	xaxis.append (a)
nxaxis=(','.join(str(e) for e in xaxis))
snip.rv=(nxaxis)
`}
\draw (\x cm,1pt) -- (\x cm, -1pt) node[anchor=north] {$\x$};
\draw[->](`!p snip.rv=match.group(5)`,`!p snip.rv=match.group(6)`) -- (`!p snip.rv=match.group(7)`,`!p snip.rv=match.group(8)`) node[anchor=south east] {$2};
\foreach \y in {`!p
yaxis=[]
y=abs(int(match.group(6)))+abs(int(match.group(8)))
for b in range(y):
	yaxis.append (b)
nyaxis=(','.join(str(e) for e in yaxis))
snip.rv=(nyaxis)
`}
\draw (1pt,\y cm) -- (-1pt,\y cm) node[anchor=east] {$\y$};
\end{tikzpicture} $0
endsnippet



